{
  "name": "structured-log",
  "version": "0.2.0",
  "description": "A structured logging framework for JavaScript, inspired by Serilog.",
  "main": "dist/indexjs",
  "jsnext:main": "dist/structured-log.es6.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "build-ts": "tsc --outDir dist",
    "build-es5": "rollup -c rollup.config.es5.js",
    "build-es6": "rollup -c rollup.config.es6.js",
    "build": "npm run clean-cache && npm run build-ts && npm run build-es5 && npm run build-es6",
    "clean-cache": "rimraf .rpt2_cache",
    "prepublish": "npm run build"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/structured-log/structured-log"
  },
  "keywords": [
    "logging",
    "structured",
    "semantic",
    "serilog"
  ],
  "author": "structured-log Contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/structured-log/structured-log/issues"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/es6-promise": "0.0.32",
    "@types/jest": "^21.1.1",
    "@types/node": "^6.0.57",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "es6-promise": "^4.0.5",
    "es6-symbol": "^3.1.0",
    "jest": "^21.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-typescript2": "^0.9.0",
    "ts-jest": "^21.0.1",
    "ts-node": "^3.0.0",
    "tslib": "^1.8.1",
    "typemoq": "^1.1.0",
    "typescript": "^2.1.4"
  }
}
